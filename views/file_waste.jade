include ./includes/header.jade

h1=title
table(class='table table-striped table-hover table-condensed')
  thead
    tr
      th Library
      th File
      th Member
      th Rows
      th Deleted rows
      th Percent deleted
      th Data space
      th Deleted space
  tbody
    - each row in results
      tr
        td=row.SYSTEM_TABLE_NAME
        td=row.SYSTEM_TABLE_SCHEMA
        td=row.SYSTEM_TABLE_MEMBER
        td=row.NUMBER_ROWS
        td=row.NUMBER_DELETED_ROWS
        td=row.PERCENT_DELETED
        td=row.DATA_SIZE
        td=row.DELETED_SPACE
          
include ./includes/footer.jade